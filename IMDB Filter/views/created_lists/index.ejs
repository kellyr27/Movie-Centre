<% layout('layouts/boilerplate') %>
<h1>Created Lists</h1>
<form action="post" ></form>
<% if (displayList.length === 0) { %>
    <p>You have no created lists!</p>
<% } else { %>
    <ol>
    <% for (let i = 0; i < displayList.length; i++) { %>
        <li><a href="/created_lists/<%= displayList[i]['id']%>"><%= displayList[i]['listName'] %></a></li>
    <% } %> 
    </ol>
<% } %> 
<a href="/created_lists/new">Create new list</a>
